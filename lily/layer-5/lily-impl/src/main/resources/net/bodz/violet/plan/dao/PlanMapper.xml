        <include refid="co.opts" />
    </select>

    <select id="filter" resultMap="objlist_map">
        <include refid="objlist_sql" />
        <where>
            <include refid="filtconds" />
        </where>
        <include refid="co.opts" />
    </select>

    <select id="select" resultMap="objlist_map">
        <include refid="objedit_sql" />
        <where>
            <if test="_parameter != null">a.id = #{id}</if>
        </where>
    </select>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id"><![CDATA[
        insert into plan(
            uid,
            gid,
            mode,
            acl,
            priority,
            flags,
            "state",
            t0,
            t1,
            "year",
            subject,
            op,
            text,
            form,
            formargs,
            creation,
            lastmod,
            "version",
            cat,
            phase,
            nread,
            nvote,
            nlike,
            "value"
        ) values(
            #{ownerUserId},
            #{ownerGroupId},
            #{accessMode},
            #{acl},
            #{priority},
            #{flags},
            #{state},
            #{beginTime},
            #{endTime},
            #{year},
            #{subject},
            #{opId},
            #{rawText},
            #{formId},
            #{formArguments},
            #{creationDate},
            #{lastModifiedDate},
            #{version},
            #{categoryId},
            #{phaseId},
            #{readCount},
            #{voteCount},
            #{nlike},
            #{value}
        );
    ]]></insert>

    <update id="update">
        update plan
        <set>
            uid = #{ownerUserId},
            gid = #{ownerGroupId},
            mode = #{accessMode},
            acl = #{acl},
            priority = #{priority},
            flags = #{flags},
            "state" = #{state},
            t0 = #{beginTime},
            t1 = #{endTime},
            "year" = #{year},
            subject = #{subject},
            op = #{opId},
            text = #{rawText},
            form = #{formId},
            formargs = #{formArguments},
            creation = #{creationDate},
            lastmod = #{lastModifiedDate},
            "version" = #{version},
            cat = #{categoryId},
            phase = #{phaseId},
            nread = #{readCount},
            nvote = #{voteCount},
            nlike = #{nlike},
            "value" = #{value},
        </set>
        <where>
            id = #{id}
        </where>
    </update>

    <delete id="delete">
        delete from plan
        <where>
            id = #{id}
        </where>
    </delete>

    <select id="count" resultType="long">
        select count(*) "rows" from plan
        <where>
            <if test="_parameter != null">
                <include refid="filtconds" />
            </if>
        </where>
    </select>

</mapper>
