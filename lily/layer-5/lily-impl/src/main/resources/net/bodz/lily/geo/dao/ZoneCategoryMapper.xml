    <sql id="objlist_sql"><![CDATA[
        select
            a.id, 
            a.code, 
            a.uid, 
            a.gid, 
            a.mode, 
            a.acl, 
            a.label, 
            a.description, 
            a.priority, 
            a.flags, 
