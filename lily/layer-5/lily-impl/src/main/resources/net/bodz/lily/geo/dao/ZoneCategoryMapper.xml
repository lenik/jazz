    <select id="all" resultMap="objlist_map">
        <include refid="objlist_sql" />
        <include refid="co.opts" />
    </select>

    <select id="filter" resultMap="objlist_map">
        <include refid="objlist_sql" />
        <where>
            <include refid="filtconds" />
        </where>
        <include refid="co.opts" />
    </select>

    <select id="select" resultMap="objlist_map">
        <include refid="objedit_sql" />
        <where>
            <if test="_parameter != null">a.id = #{id}</if>
        </where>
    </select>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id"><![CDATA[
        insert into zonecat(
            code,
            uid,
            gid,
            mode,
            acl,
            label,
            description,
            priority,
            flags,
            "state",
            creation,
            lastmod,
            "version",
            image,
            imagealt,
            props,
            parent,
            "depth"
        ) values(
            #{codeName},
            #{uId},
            #{gId},
            #{mode},
            #{acl},
            #{label},
            #{description},
            #{priority},
            #{flags},
            #{state},
            #{creationDate},
            #{lastModifiedDate},
            #{version},
            #{image},
            #{imageAlt},
            #{properties.jsonStr}::jsonb,
            #{parentId},
            #{depth}
        );
    ]]></insert>

    <update id="update">
        update zonecat
        <set>
            code = #{codeName},
            uid = #{uId},
            gid = #{gId},
            mode = #{mode},
            acl = #{acl},
            label = #{label},
            description = #{description},
            priority = #{priority},
            flags = #{flags},
            "state" = #{state},
            creation = #{creationDate},
            lastmod = #{lastModifiedDate},
            "version" = #{version},
            image = #{image},
            imagealt = #{imageAlt},
            props = #{properties.jsonStr}::jsonb,
            parent = #{parentId},
            "depth" = #{depth},
        </set>
        <where>
            id = #{id}
        </where>
    </update>

    <delete id="delete">
        delete from zonecat
        <where>
            id = #{id}
        </where>
    </delete>

    <select id="count" resultType="long">
        select count(*) "rows" from zonecat
        <where>
            <if test="_parameter != null">
                <include refid="filtconds" />
            </if>
        </where>
    </select>

    <update id="delete_xrefs">
        alter table lily."zone" drop constraint zone_cat_fkey;
        alter table lily.zonecat drop constraint zonecat_parent_fkey;
    </update>

    <update id="create_xrefs">
        alter table lily."zone"
            add constraint zone_cat_fkey foreign key (cat)
            references lily.zonecat (id)
            on update cascade
            ;
        alter table lily.zonecat
            add constraint zonecat_parent_fkey foreign key (parent)
            references lily.zonecat (id)
            on update cascade
            on delete cascade
            ;
    </update>

</mapper>
