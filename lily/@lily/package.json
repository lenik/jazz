{
    "name": "lily",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite --host 0.0.0.0",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force"
    },
    "dependencies": {
        "add": "^2.0.6",
        "bootstrap": "^4.6.2",
        "clipboard": "^2.0.11",
        "font-awesome": "^4.7.0",
        "jquery": "^3.7.1",
        "jquery-ui": "^1.13.2",
        "pinia": "^2.1.7",
        "sass": "^1.69.6",
        "vue": "^3.4.0",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@tsconfig/node18": "^18.2.2",
        "@types/jquery": "^3.5.29",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^18.19.3",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/test-utils": "^2.4.3",
        "@vue/tsconfig": "^0.5.1",
        "jsdom": "^23.0.1",
        "npm-run-all2": "^6.1.1",
        "typescript": "~5.3.3",
        "vite": "^5.0.10",
        "vitest": "^1.1.0",
        "vue-tsc": "^1.8.27"
    },
    "workspaces": [
        "package/*"
    ],
    "rules": {
        "no-unused-vars": "off"
    },
    "pnpm": {
        "overrides": {
            "rollup": "npm:@rollup/wasm-node"
        }
    }
}
