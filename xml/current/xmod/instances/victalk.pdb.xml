<?xml version="1.0" encoding="UTF-8"?>
<modpdb xmlns="http://www.bodz.net/xml/current/xmod/modpdb" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bodz.net/xml/current/xmod/modpdb
C:\.radiko\.miaj\mirror\site\xml\current\xmod\modpdb.xsd" cat="biz/support/spy" name="VicTalk" qname="com.ebja.victalk" disp="Successful Negotiation Support" info="成功谈判支持">
	<table name="Prp" opts="D" disp="属性" info="量化属性类别。">
		<field name="id" disp="id/prp" type="I" opts="KDi"/>
		<field name="cat" disp="属性分类" type="C" opts="I" info="'*'=通用
'S'=语句适用
'T'=目标适用"/>
		<field name="name" disp="名称" type="C[,30]" opts="I"/>
		<field name="type" disp="数据类型" type="C" info="'I'=integer
'F'=float
'D'=decibel"/>
		<field name="min" disp="最小值" type="F" info="取值范围"/>
		<field name="max" disp="最大值" type="F" info="取值范围"/>
		<field name="info" disp="帮助信息" type="C[]"/>
		<row>
			<cat>*</cat>
			<name>说服</name>
			<type>R</type>
			<min>-10</min>
			<max>10</max>
			<info>沦陷 -> 说服</info>
		</row>
		<row>
			<cat>*</cat>
			<name>转移注意力</name>
			<type>R</type>
			<min>-10</min>
			<max>10</max>
		</row>
		<row>
			<cat>*</cat>
			<name>跟进</name>
			<type>R</type>
		</row>
		<row>
			<cat>*</cat>
			<type>R</type>
		</row>
	</table>
	<table name="Ctx" disp="上下文" info="谈判情景。
默认情景(0)表示未识别的情景。">
		<field name="id" disp="id/ctx" type="I" opts="KDi"/>
		<field name="desc" disp="简要描述" type="C[,100]" opts="I"/>
		<field name="detail" disp="详细描述" type="C[]"/>
		<field name="gparent" disp="一般父上下文" type="I" opts="F(n:1-->Ctx.id)
D(0)"/>
		<row>
			<id>0</id>
			<desc>未识别</desc>
			<detail>未识别的上下文</detail>
			<gparent>0</gparent>
		</row>
	</table>
	<table name="CtxR" disp="上下文关系" info="从某一情景至另一情景的时序统计关系。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="a" disp="情景一" type="I" opts="F(n:1>>Ctx.id)"/>
		<field name="b" disp="情景二" type="I" opts="F(n:1>>Ctx.id)"/>
		<field name="count" disp="实证计数器" type="I" opts="D(0)" info="某当实际对话中从情景一转移至情景二发生时，即加1。"/>
	</table>
	<table name="Tgt" disp="目标" info="谈判的目标。
默认目标(0)表示未识别的目标，用于速记候选语句。">
		<field name="id" disp="id/tgt" type="I" opts="KDi"/>
		<field name="desc" disp="简要描述" type="C[,100]" opts="I"/>
		<field name="detail" disp="详细描述" type="C[]"/>
		<row>
			<id>0</id>
			<desc>未识别</desc>
			<detail>未识别的目标</detail>
		</row>
	</table>
	<table name="TP" disp="目标属性" info="目标关注的属性。
谈判">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="prp" disp="属性类型" type="I" opts="F(n:1->Prp.id)"/>
		<field name="val" disp="属性值" type="F"/>
	</table>
	<table name="Stm" disp="语句" info="语句内容。">
		<field name="id" disp="id/stm" type="I" opts="KDi"/>
		<field name="txt" disp="语句文本" type="C[,200]" opts="I"/>
		<field name="full" disp="完整的语句文本" type="C[]"/>
	</table>
	<table name="StmR" disp="语句关系" info="从某一语句接续至另一语句的时序统计关系。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="a" disp="语句一" type="I" opts="F(n:1>>Stm.id)"/>
		<field name="b" disp="语句二" type="I" opts="F(n:1>>Stm.id)"/>
		<field name="count" disp="实证计数器" type="I" opts="D(0)" info="某当实际对话中从语句一接续至语句二发生时，即加1。"/>
	</table>
	<table name="SP" disp="语句属性" info="语句的量化属性。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="prp" disp="属性类型" type="I" opts="F(n:1->Prp.id)"/>
		<field name="val" disp="属性值" type="F"/>
	</table>
	<table name="Ch" disp="角色类型" info="通用角色类型。">
		<field name="id" disp="id/ch" type="I" opts="KDi"/>
		<field name="name" disp="名称" type="C[,30]" opts="I"/>
		<field name="desc" disp="描述信息" type="C[,200]"/>
	</table>
	<table name="Dlg" disp="对话实例" info="实时录制的对话实例。">
		<field name="id" disp="id/dlg" type="I" opts="KDi"/>
		<field name="t0" disp="对话起始时间" type="T" opts="I"/>
		<field name="dur" disp="对话持续时间(秒)" type="F" opts="D(0)"/>
		<field name="cat" disp="分类" type="C[,50]" opts="D('')"/>
		<field name="title" disp="对话标题" type="C[,100]" opts="I"/>
		<field name="memo" disp="备忘录" type="C[]"/>
	</table>
	<table name="DlgCh" disp="对话角色" info="参与对话实例的角色实例。">
		<field name="id" disp="id/dch" type="I" opts="KDi"/>
		<field name="dlg" disp="对话实例" type="I" opts="F(n:1>>Dlg.id)"/>
		<field name="ch" disp="角色类型" type="I" opts="F(n:1->Ch.id)"/>
		<field name="name" disp="实例名称" type="C[,30]" opts="D('?')" info="扮演角色者的真实姓名。"/>
		<field name="act" disp="是否主动" type="Bt" opts="D(0)" info="是否为主动方（Nevic系统可控）。"/>
	</table>
	<table name="DlgRes" disp="对话资源" info="实时录制的对话资源。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="dlg" disp="对话实例" type="I" opts="F(n:1>>Dlg.id)"/>
		<field name="off" disp="时间偏移(秒)" type="F" opts="D(0)"/>
		<field name="href" disp="资源链接" type="C[,200]"/>
	</table>
	<table name="DlgCtx" disp="对话上下文" info="对话实例的上下文转移序列。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="dlg" disp="对话实例" type="I" opts="F(n:1>>Dlg.id)"/>
		<field name="off" disp="时间偏移(秒)" type="F" opts="D(0)"/>
		<field name="ctx" disp="已定义上下文" type="I" opts="F(n:1->Ctx.id)"/>
		<field name="memo" disp="备注" type="C[]"/>
	</table>
	<table name="DlgStm" disp="对话语句" info="对话实例的语句实例序列。">
		<field name="id" disp="id/dstm" type="I" opts="KDi"/>
		<field name="dlg" disp="对话实例" type="I" opts="F(n:1>>Dlg.id)"/>
		<field name="off" disp="时间偏移(秒)" type="F" opts="D(0)"/>
		<field name="dch" disp="扮演者" type="I" opts="F(n:1->DlgCh.id)"/>
		<field name="stm" disp="已定义语句" type="I" opts="F(n:1->Stm.id)"/>
		<field name="memo" disp="备注" type="C[]"/>
	</table>
	<table name="DlgSP" disp="对话属性评定" info="针对某一实例对话的属性评定。
评定结果作为语句属性的量化依据。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="dstm" disp="语句实例" type="I" opts="F(n:1>>DlgStm.id)"/>
		<field name="prp" disp="属性类型" type="I" opts="F(n:1->Prp.id)"/>
		<field name="val" disp="属性值" type="F"/>
	</table>
	<table name="RefT" opts="DRo" disp="参考源类型" info="对参考信息数据源的分类。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="name" disp="名称" type="C[,30]" opts="I"/>
		<field name="is_href" disp="是否链接" type="Bt"/>
		<row>
			<name>文档</name>
			<is_href>1</is_href>
		</row>
		<row>
			<name>图像</name>
			<is_href>1</is_href>
		</row>
		<row>
			<name>声音</name>
			<is_href>1</is_href>
		</row>
		<row>
			<name>剪辑</name>
			<is_href>1</is_href>
		</row>
	</table>
	<table name="Ref" disp="参考源" info="参考信息的数据源。
可能是内含的数据或外部数据的链接。">
		<field name="id" disp="id" type="I" opts="KDi"/>
		<field name="t" disp="类型" type="I" opts="F(n:1->RefT.id)"/>
		<field name="href" disp="链接" type="C[,200]" opts="N"/>
		<field name="dat" disp="内含数据" type="C[]" opts="N"/>
	</table>
</modpdb>
