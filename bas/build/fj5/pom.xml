    <name>BAS :: Build :: Fj5</name>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
                <version>1.4</version>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                        <configuration>
                            <artifactSet>
                                <includes>
                                    <include>net.bodz.bas:*</include>
                                </includes>
                            </artifactSet>
                            <filters>
                                <filter>
                                    <artifact>*:*</artifact>
                                    <excludes>
                                        <exclude>**/.keep</exclude>
                                    </excludes>
                                </filter>
                            </filters>
                            <relocations><!-- <relocation> --><!-- <pattern>net.bodz.bas.util.exception</pattern> --><!-- <shadedPattern>javax.free</shadedPattern> --><!-- </relocation> --></relocations>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
